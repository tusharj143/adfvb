{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-VB9B-ETL-DEV"
		},
		"LS_AZSQL_DB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AZSQL_DB'"
		},
		"ls_blob_event_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_event'"
		},
		"ls_csv_adlsraw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_csv_adlsraw'"
		},
		"ls_csv_adlssrc_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_csv_adlssrc'"
		},
		"ls_sftp_pq_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sftp_pq'"
		},
		"ls_snow_pq_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_snow_pq'"
		},
		"ls_sql_pq_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sql_pq'"
		},
		"LS_AZSQL_DB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-servr-vb-dev.database.windows.net"
		},
		"LS_AZSQL_DB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-db-vb-dev"
		},
		"LS_AZSQL_DB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminvb"
		},
		"ls_csv_adlsraw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdevvisionboardraw1.dfs.core.windows.net/"
		},
		"ls_csv_adlssrc_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsdevvbsource001.dfs.core.windows.net/"
		},
		"ls_rest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://retoolapi.dev"
		},
		"ls_sftp_pq_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "adlsdevvisionboardraw1.blob.core.windows.net"
		},
		"ls_sftp_pq_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adlsdevvisionboardraw1.sftpuser"
